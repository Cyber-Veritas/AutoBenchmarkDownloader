<Application x:Class="AutoBenchmarkDownloader.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AutoBenchmarkDownloader"
             xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
             xmlns:mica="clr-namespace:MicaWPF.Styles;assembly=MicaWPF"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ui:ThemesDictionary Theme="Dark" />
                <ui:ControlsDictionary />
                <mica:ThemeDictionary DesignModeTheme="Dark" Theme="Auto" />
                <mica:ControlsDictionary />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
